{
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "func-names": 0,
        "global-require": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "no-restricted-globals": 0
    }
}